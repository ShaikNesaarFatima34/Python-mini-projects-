    "    def min(numberlist):\n",
    "        temp=0\n",
    "        for x in numberlist:\n",
    "            if x>temp:\n",
    "                temp=x\n",
    "        for y in numberlist:\n",
    "            if y<temp:\n",
    "                temp=y\n",
    "        print(f'the min value is{temp}')\n",
    "              \n",
